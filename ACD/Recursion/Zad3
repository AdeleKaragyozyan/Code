using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace zad1
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int n = int.Parse(Console.ReadLine().Replace("reverse(","").Replace(")",""));
            Console.WriteLine(Reverse(n));
        }

        static int Reverse(int n, int reversedN=0)
        {
            if (n == 0)
                return reversedN;
            else return Reverse(n/10,reversedN*10+n%10);
        }
    }
}
